    "react-sortable-hoc": "^2.0.0",
    "sonner": "^2.0.7",
    "supabase": "^2.72.1",
    "tailwind-merge": "^3.4.0",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@heroicons/react": "^2.2.0",
