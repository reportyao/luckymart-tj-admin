{
  "name": "luckymart-tj-admin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "build:check": "npm run lint && npm run type-check && npm run build",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run 'src/**/*.test.ts' 'src/**/*.test.tsx'",
    "test:integration": "vitest --run 'src/**/*.integration.test.ts' 'src/**/*.integration.test.tsx'",
    "test:all": "vitest --run",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "start": "node server.js"
  },
